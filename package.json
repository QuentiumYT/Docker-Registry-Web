  },
  "bugs": {
    "url": "https://github.com/QuentiumYT/Docker-Registry-Web/issues"
  },
  "scripts": {
    "start": "node_modules/.bin/webpack --mode development --watch",
    "hot": "node_modules/.bin/webpack-dev-server --progress --mode development",
